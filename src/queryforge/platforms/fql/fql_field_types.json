        "indexed": true
      },
      "registry_key": {
        "type": "string",
        "description": "Windows registry key path",
        "indexed": true
      },
      "registry_value": {
        "type": "string",
        "description": "Windows registry value name",
        "indexed": true
      },
      "severity": {
        "type": "number",
        "description": "Event severity level",
        "indexed": true
      },
      "tags": {
        "type": "array",
        "description": "Event tags or labels",
        "indexed": true
      }
    }
  }
}
