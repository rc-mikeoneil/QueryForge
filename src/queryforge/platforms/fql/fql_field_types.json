{
  "field_types": {
    "string": {
      "description": "Text field",
      "compatible_operators": ["=", "!=", "contains", "startswith", "endswith", "in", "not in", "=~", "!~", "*", "exists", "not exists"],
      "examples": ["process_name", "command_line", "file_path", "user_name"]
    },
    "number": {
      "description": "Numeric field (integer or float)",
      "compatible_operators": ["=", "!=", ">", ">=", "<", "<=", "in", "not in", "exists", "not exists"],
      "examples": ["port", "pid", "count", "severity"]
    },
    "boolean": {
      "description": "Boolean field (true/false)",
      "compatible_operators": ["=", "!=", "exists", "not exists"],
      "examples": ["is_admin", "is_encrypted", "success"]
    },
    "ip": {
      "description": "IP address field (IPv4 or IPv6)",
      "compatible_operators": ["=", "!=", "in", "not in", "exists", "not exists"],
      "examples": ["source_ip", "destination_ip", "remote_ip"]
    },
    "datetime": {
      "description": "Timestamp field",
      "compatible_operators": ["=", "!=", ">", ">=", "<", "<=", "exists", "not exists"],
      "examples": ["event_time", "created_timestamp", "modified_time"]
    },
    "array": {
      "description": "Array/list field",
      "compatible_operators": ["contains", "in", "exists", "not exists"],
      "examples": ["tags", "labels", "file_hashes"]
    }
  },
  "placeholder_fields": {
    "comment": "Placeholder fields until real FQL schema is available",
    "events": {
      "@timestamp": {
        "type": "datetime",
        "description": "Event timestamp",
        "indexed": true
      },
      "event_type": {
        "type": "string",
        "description": "Type of event",
        "indexed": true
      },
      "host": {
        "type": "string",
        "description": "Hostname or device name",
        "indexed": true
      },
      "process_name": {
        "type": "string",
        "description": "Name of the process",
        "indexed": true
      },
      "process_id": {
        "type": "number",
        "description": "Process ID (PID)",
        "indexed": true
      },
      "parent_process_name": {
        "type": "string",
        "description": "Name of parent process",
        "indexed": true
      },
      "parent_process_id": {
        "type": "number",
        "description": "Parent process ID (PPID)",
        "indexed": true
      },
      "command_line": {
        "type": "string",
        "description": "Full command line",
        "indexed": true
      },
      "user_name": {
        "type": "string",
        "description": "Username associated with event",
        "indexed": true
      },
      "file_path": {
        "type": "string",
        "description": "Full file path",
        "indexed": true
      },
      "file_name": {
        "type": "string",
        "description": "File name only",
        "indexed": true
      },
      "file_hash": {
        "type": "string",
        "description": "File hash (MD5/SHA1/SHA256)",
        "indexed": true
      },
      "source_ip": {
        "type": "ip",
        "description": "Source IP address",
        "indexed": true
      },
      "destination_ip": {
        "type": "ip",
        "description": "Destination IP address",
        "indexed": true
      },
      "destination_port": {
        "type": "number",
        "description": "Destination port number",
        "indexed": true
      },
      "protocol": {
        "type": "string",
        "description": "Network protocol",
        "indexed": true
      },
      "http_user_agent": {
        "type": "string",
